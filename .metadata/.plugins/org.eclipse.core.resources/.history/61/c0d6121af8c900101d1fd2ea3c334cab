package com.controlStatment;

public class LeapYearOrNot {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int year = 2016;
        if(year % 4 == 0)
        {
        	System.out.println("Year is Leap year");
        }
        else
        {
        	System.out.println("Year is not a Leap year");
        }
	}

}
